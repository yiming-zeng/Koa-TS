{
    "verbose": false,
    "debug": false,
    "exec": "ts-node ./app.ts",
    "ignore": [
        "node_modules",
        "./test",
        "**/*.d.ts",
        "*.test.ts",
        "*.spec.ts",
        "fixtures/*",
        "test/**/*",
        "docs/*"
    ],
    "events": {
        "restart": ""
    },
    "watch": [
        "./app",
        "./configs"
    ],
    "ext": "ts",
    "inspect": true
}